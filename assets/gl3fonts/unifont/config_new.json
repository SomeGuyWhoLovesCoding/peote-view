{
	"packing": 1, // 0 - no packing, 1 - packed with ttfcompile of gl3font (see here: https://github.com/deltaluca/gl3font) 
	"distancefield": true, // use ttfcompile do generate distancefields
	
	"kerning": false,
	"width" : 16,
	"height": 16,
